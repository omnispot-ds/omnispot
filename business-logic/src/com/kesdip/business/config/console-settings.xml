<?xml version="1.0" encoding="UTF-8"?>

<settings>

	<file-storage>
		<ps-root-folder>c:/tmp/console/print-screens/</ps-root-folder>
		<content-root-folder>c:/tmp/console/content/</content-root-folder>
		<printScreen-name>printScreen.jpg</printScreen-name>
		<default-printScreen>c:/tmp/console/print-screens/defaultPrintScreen.jpg</default-printScreen>
		<temp-folder>c:/tmp/temp-files</temp-folder>
	</file-storage>

	<server>
		<content-base>http://192.168.1.4:8080/admin-console/content/</content-base>
		<printScreen-base>http://192.168.1.4:8080/admin-console/secure/printScreen/</printScreen-base>
	</server>
	
</settings>